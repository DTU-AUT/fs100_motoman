#!/usr/bin/python2

import sys, thread, threading, time, socket, struct, getopt, math




def mainProgram(host):
    msg_notify = threading.Condition() #"semaphore"
    robot = basicUrComm(host, msg_notify)
    robot.start()
    time.sleep(1)
    print "getting joint values"
    joints = robot.get_joints()
    print str(joints)
    time.sleep(2)
    print "moving to home"
    robot.move_to_start()
    print "getting joint values"
    joints = robot.get_joints()
    print str(joints)
    print "program done, exitting"

     

if __name__ == "__main__":
	mainProgram()
